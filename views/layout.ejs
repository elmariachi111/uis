            <img src="<%=user.thumbUrl%>" />
            <% } else { %>
            <h1>?</h1>
            <% } %>
        </div>
        <div id="imgLogo">
            <img src="/images/logo_big.png" />
        </div>
        <div id="header-bg">

            <div id="btn-panel" align="center">
                <div class="btn" id="btn-show-favs">
                    <a href="/stream/favorites"><img src="/images/heart.png" /></a>
                </div>
                <div class="btn" id="btn-show-friends">
                    <a href="/stream/friends"><img src="/images/friends.png" /></a>
                </div>
            </div>
        </div>

    </div>
